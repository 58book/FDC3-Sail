{
  "name": "electron-fdc3",
  "private": true,
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "start": "npm run watch",
    "build": "npm run build:main && npm run build:preload && npm run build:view-preload  && npm run build:channelPicker-preload  && npm run build:intentResolver-preload  && npm run build:searchResults-preload && npm run build:renderer && npm run build:view && npm run build:channelPicker && npm run build:intentResolver && npm run build:searchResults",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:view-preload": "cd ./packages/view-preload && vite build",
    "build:channelPicker-preload": "cd ./packages/channelPicker-preload && vite build",
    "build:intentResolver-preload": "cd ./packages/intentResolver-preload && vite build",
    "build:searchResults-preload": "cd ./packages/searchResults-preload && vite build",
    "build:preload:types": "dts-cb -i \"packages/preload/src/**/*.ts\" -o \"packages/preload/exposedInMainWorld.d.ts\"",
    "build:view-preload:types": "dts-cb -i \"packages/view-preload/src/**/*.ts\" -o \"packages/view-preload/exposedInMainWorld.d.ts\"",
    "build:channelPicker-preload:types": "dts-cb -i \"packages/channelPicker-preload/src/**/*.ts\" -o \"packages/channelPicker-preload/exposedInMainWorld.d.ts\"",
    "build:intentResolver-preload:types": "dts-cb -i \"packages/intentResolver-preload/src/**/*.ts\" -o \"packages/intentResolver-preload/exposedInMainWorld.d.ts\"",
    "build:searchResults-preload:types": "dts-cb -i \"packages/searchResults-preload/src/**/*.ts\" -o \"packages/searchResults-preload/exposedInMainWorld.d.ts\"",
    "build:renderer": "cd ./packages/renderer && vite build",
    "build:view": "cd ./packages/view && vite build",
    "build:channelPicker": "cd ./packages/channelPicker && vite build",
    "build:intentResolver": "cd ./packages/intentResolver && vite build",
    "build:searchResults": "cd ./packages/searchResults && vite build",
    "compile": "cross-env MODE=production npm run build && electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "test": "npm run test:main && npm run test:preload && npm run test:renderer && npm run test:view  && npm run test:view-preload  && npm run test:channelPicker  && npm run test:channelPicker-preload  && npm run test:intentResolver  && npm run test:intentResolver-preload  && npm run test:searchResults  && npm run test:searchResults-preload && npm run test:e2e",
    "test:e2e": "npm run build && vitest run",
    "test:main": "vitest run -r packages/main --passWithNoTests",
    "test:preload": "vitest run -r packages/preload --passWithNoTests",
    "test:view-preload": "vitest run -r packages/view-preload --passWithNoTests",
    "test:renderer": "vitest run -r packages/renderer --passWithNoTests",
    "test:view": "vitest run -r packages/view --passWithNoTests",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,ts,tsx",
    "typecheck:main": "tsc --noEmit -p packages/main/tsconfig.json",
    "typecheck:preload": "tsc --noEmit -p packages/preload/tsconfig.json",
    "typecheck:view-preload": "tsc --noEmit -p packages/view-preload/tsconfig.json",
    "typecheck:channelPicker-preload": "tsc --noEmit -p packages/channelPicker-preload/tsconfig.json",
    "typecheck:intentResolver-preload": "tsc --noEmit -p packages/intentResolver-preload/tsconfig.json",
    "typecheck:searchResults-preload": "tsc --noEmit -p packages/searchResults-preload/tsconfig.json",
    "typecheck:renderer": "npm run build:preload:types && tsc --noEmit -p packages/renderer/tsconfig.json",
    "typecheck:view": "npm run build:view-preload:types && tsc --noEmit -p packages/view/tsconfig.json",
    "typecheck:channelPicker": "npm run build:channelPicker-preload:types && tsc --noEmit -p packages/channelPicker/tsconfig.json",
    "typecheck:intentResolver": "npm run build:intentResolver-preload:types && tsc --noEmit -p packages/intentResolver/tsconfig.json",
    "typecheck:searchResults": "npm run build:searchResults-preload:types && tsc --noEmit -p packages/searchResults/tsconfig.json",
    "typecheck": "npm run typecheck:main && npm run typecheck:preload && npm run typecheck:view-preload && npm run typecheck:renderer && npm run typecheck:view && npm run typecheck:channelPicker-preload && npm run typecheck:intentResolver-preload && npm run typecheck:searchResults-preload && npm run typecheck:channelPicker && npm run typecheck:intentResolver && npm run typecheck:searchResults",
    "update-versions": "node scripts/update-electron-vendors.js"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/node": "^17.0.6",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@vitejs/plugin-react": "^1.1.3",
    "cross-env": "^7.0.3",
    "dts-for-context-bridge": "^0.7.1",
    "electron": "^17.0.0",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jsdom": "^19.0.0",
    "nano-staged": "^0.5.0",
    "playwright": "^1.19.2",
    "prettier": "^2.5.1",
    "simple-git-hooks": "^2.7.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "vitest": "^0.2.0"
  },
  "dependencies": {
    "@finos/fdc3": "^1.2.0",
    "@fontsource/roboto": "^4.5.3",
    "@tarekraafat/autocomplete.js": "^10.2.6",
    "electron-fetch": "^1.7.4",
    "electron-updater": "^4.6.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
